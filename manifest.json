{
  "manifest_version": 2,
  "name": "Tab Count in Window Title",
  "version": "3.0",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "strict_min_version": "56.0"
    }
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  }
}